<div class="row">
  <content class="col-md-9" role="main">
    <div class="page-header">
      <h1>Settings</h1>
    </div>

    <div class="col-md-6">
      <h3>Debug</h3>

      <table class="table table-striped">
        <thead>
          <tr>
            <th>ENV</th>
            <th>Value</th>
          </tr>
        </thead>
        <tbody>
          <% @settings.each do |k, v| -%>
            <tr>
              <th><b><%= k %></b></th>
              <td>
                <%= v %>
              </td>
            </tr>
          <% end -%>
        </tbody>
      </table>
    </div>
  </content>
</div>
